// Linked list - Singly

class Node{
    int value;
    Node next;
    
    // constructor 
    
    Node(int value){
        this.value = value;
        this.next = null;
    }
}

public class SLL{
    
    Node head;
    
    
    // InsertAtBeginning 
    
    public void InsertAtBeginning(int value){
        Node newNode = new Node(value);
        newNode.next = head;
        head=newNode;
    } 
    
    // display method 
    
    public void display(){
     
       Node temp = head;
       while(temp!=null){
           System.out.print(temp.value + " ->");
           temp = temp.next;
       }
    
       System.out.print("null");
    }
    public static void main(String[] args){
        
        SLL list = new SLL();
        list.InsertAtBeginning(100);
        list.InsertAtBeginning(200);
        list.InsertAtBeginning(300);
        list.InsertAtBeginning(400);
        
        
        list.display();
    }
}
